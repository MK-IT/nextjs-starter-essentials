{
  "name": "nextjs-starter-essentials",
  "version": "2.3.1",
  "description": "NextJS starter bundled with essential tools",
  "keywords": [
    "nextjs",
    "starter",
    "essentials",
    "mkit"
  ],
  "scripts": {
    "prepare": "husky install",
    "gen": "HYGEN_TMPLS=.hygen hygen",
    "gen:component": "HYGEN_TMPLS=.hygen hygen react component",
    "gen:page": "HYGEN_TMPLS=.hygen hygen react page",
    "gen:hook": "HYGEN_TMPLS=.hygen hygen react hook",
    "gen:utils": "HYGEN_TMPLS=.hygen hygen react utils",
    "gen:api": "HYGEN_TMPLS=.hygen hygen nextjs api",
    "format": "prettier --write src",
    "lint:fs": "ls-lint",
    "lint": "eslint --fix src",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "dev": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "start": "next start",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "analyze": "ANALYZE=true yarn build",
    "release": "standard-version"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^12.1.4",
    "next": "12.1.0",
    "next-seo": "^5.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass": "^1.49.9"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@ls-lint/ls-lint": "^1.11.0",
    "@next/eslint-plugin-next": "^12.1.0",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.41",
    "@types/react-dom": "^17.0.14",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.15.0",
    "eslint": "^8.12.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "16.1.4",
    "eslint-config-next": "^12.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-testing-library": "^5.1.0",
    "husky": "^7.0.4",
    "hygen": "^6.2.0",
    "jest": "^27.5.1",
    "next-sitemap": "^2.5.10",
    "prettier": "^2.6.1",
    "standard-version": "^9.3.2",
    "typescript": "4.6.2"
  },
  "author": "MK IT Ltd. <hi@mkit.io>",
  "homepage": "https://github.com/MK-IT/nextjs-starter-essentials",
  "repository": {
    "type": "git",
    "url": "https://github.com/MK-IT/nextjs-starter-essentials.git"
  },
  "bugs": {
    "url": "https://github.com/MK-IT/nextjs-starter-essentials/issues"
  },
  "license": "MIT"
}

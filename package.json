{
  "name": "nextjs-starter-essentials",
  "version": "2.3.1",
  "description": "NextJS starter bundled with essential tools",
  "keywords": [
    "nextjs",
    "starter",
    "essentials",
    "mkit"
  ],
  "scripts": {
    "prepare": "husky install",
    "gen": "HYGEN_TMPLS=.hygen hygen",
    "gen:component": "HYGEN_TMPLS=.hygen hygen react component",
    "gen:page": "HYGEN_TMPLS=.hygen hygen react page",
    "gen:hook": "HYGEN_TMPLS=.hygen hygen react hook",
    "gen:utils": "HYGEN_TMPLS=.hygen hygen react utils",
    "gen:api": "HYGEN_TMPLS=.hygen hygen nextjs api",
    "format": "prettier --write src",
    "lint:fs": "ls-lint",
    "lint": "eslint --fix src",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "dev": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "start": "next start",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "analyze": "ANALYZE=true yarn build",
    "release": "standard-version",
    "storybook": "start-storybook -p 6006 -s public",
    "storybook:build": "build-storybook -s public"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^13.0.5",
    "next": "13.0.5",
    "next-seo": "^5.15.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.56.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@ls-lint/ls-lint": "^1.11.2",
    "@next/eslint-plugin-next": "^13.0.5",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/addon-viewport": "^6.5.13",
    "@storybook/builder-webpack5": "^6.5.13",
    "@storybook/jest": "^0.0.10",
    "@storybook/manager-webpack5": "^6.5.13",
    "@storybook/react": "^6.5.13",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.47.1",
    "eslint": "^8.28.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "^13.0.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.7",
    "eslint-plugin-testing-library": "^5.9.1",
    "husky": "^8.0.2",
    "hygen": "^6.2.11",
    "jest": "^29.3.1",
    "next-sitemap": "^3.1.32",
    "prettier": "^2.8.0",
    "standard-version": "^9.5.0",
    "storybook-addon-next": "^1.7.1",
    "typescript": "^4.9.3"
  },
  "author": "MK IT Ltd. <hi@mkit.io>",
  "homepage": "https://github.com/MK-IT/nextjs-starter-essentials",
  "repository": {
    "type": "git",
    "url": "https://github.com/MK-IT/nextjs-starter-essentials.git"
  },
  "bugs": {
    "url": "https://github.com/MK-IT/nextjs-starter-essentials/issues"
  },
  "license": "MIT"
}

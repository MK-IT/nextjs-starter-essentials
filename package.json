{
  "name": "nextjs-starter-essentials",
  "version": "2.0.0",
  "description": "NextJS starter bundled with essential tools",
  "keywords": [
    "nextjs",
    "starter",
    "essentials",
    "mkit"
  ],
  "scripts": {
    "prepare": "husky install",
    "gen": "HYGEN_TMPLS=.hygen hygen",
    "gen:component": "HYGEN_TMPLS=.hygen hygen react component",
    "gen:page": "HYGEN_TMPLS=.hygen hygen react page",
    "gen:hook": "HYGEN_TMPLS=.hygen hygen react hook",
    "gen:utils": "HYGEN_TMPLS=.hygen hygen react utils",
    "gen:api": "HYGEN_TMPLS=.hygen hygen nextjs api",
    "format": "prettier --write src",
    "lint:fs": "ls-lint",
    "lint": "eslint --fix src",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "dev": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "start": "next start",
    "analyze": "ANALYZE=true yarn build",
    "release": "standard-version"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^12.0.7",
    "next": "12.0.8",
    "next-seo": "^4.28.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass": "^1.47.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.0.0",
    "@ls-lint/ls-lint": "^1.10.0",
    "@next/eslint-plugin-next": "^12.0.7",
    "@types/react": "^17.0.38",
    "@types/node": "^17.0.9",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.9.0",
    "eslint": "^8.7.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "16.1.0",
    "eslint-config-next": "^12.0.8",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "hygen": "^6.1.0",
    "next-sitemap": "^1.6.261",
    "prettier": "^2.5.1",
    "standard-version": "^9.3.2",
    "typescript": "4.5.4"
  },
  "author": "MK IT Ltd. <hi@mkit.io>",
  "homepage": "https://github.com/MK-IT/nextjs-starter-essentials",
  "repository": {
    "type": "git",
    "url": "https://github.com/MK-IT/nextjs-starter-essentials.git"
  },
  "bugs": {
    "url": "https://github.com/MK-IT/nextjs-starter-essentials/issues"
  },
  "license": "MIT"
}
